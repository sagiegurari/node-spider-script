
func TimeMachine(pilot) {
  this.pilot = pilot;

  this.go = func (noise) {
    console.log(this.pilot, noise);
    return this.pilot + ' ' + noise;
  };
}

module.exports = {
    create: func(pilot) {
        return new TimeMachine(pilot);
    }
};

return {
    create: func(pilot) {
        return new TimeMachine(pilot);
    }
};
